{
  "expo": {
    "name": "Express-Gaz",
    "slug": "Express-Gaz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/express-gaz.png",
    "scheme": "gaz",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "L'application a besoin d'accéder à votre position pour la géolocalisation.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.robert10.ExpressGaz"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/express-gaz.png",
        "backgroundColor": "#FFFFFF",
        "monochromeImage": "./assets/images/express-gaz.png"
      },
      "package": "com.robert10.ExpressGaz",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/express-gaz.png",
          "backgroundColor": "#FFFFFF",
          "resizeMode": "contain"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Autorisez $(PRODUCT_NAME) à accéder à vos photos.",
          "cameraPermission": "Autorisez $(PRODUCT_NAME) à utiliser votre caméra."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Autorisez $(PRODUCT_NAME) à utiliser votre position."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "83e6a396-2fab-4c55-9dbc-05dc17e87620"
      }
    }
  }
}
